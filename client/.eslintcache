item._id);\n      }\n    });\n\n    getCheckout({\n      variables: { products: productIds }\n    });    \n  }\n\n  \n  if (!state.cartOpen) {\n    return (\n      <div className=\"cart-closed\" onClick={toggleCart}>\n        <span\n          role=\"img\"\n          aria-label=\"trash\">üõí</span>\n      </div>\n    );\n  }\n  \n  return (\n    <div className=\"cart\">\n      <div className=\"close\" onClick={toggleCart}>[close]</div>\n      <h2>Shopping Cart</h2>\n      {state.cart.length ? (\n        <div>\n          {state.cart.map(item => (\n            <CartReservation key={item._id} item={item} />\n          ))}\n          <div className=\"flex-row space-between\">\n            <strong>Total: ${calculateTotal()}</strong>\n            {\n              Auth.loggedIn() ?\n                <button onClick={submitCheckout}>\n                  Checkout\n                </button>\n                :\n                <span>(log in to check out)</span>\n            }\n          </div>\n        </div>\n      ) : (\n        <h3>\n          <span role=\"img\" aria-label=\"shocked\">\n            üò±\n          </span>\n          You haven't added anything to your cart yet!\n        </h3>\n      )}\n    </div>\n\n  );\n};\n\nexport default Cart;\n","C:\\Users\\titol\\Desktop\\projects\\paw-sible_pet\\client\\src\\components\\CategoryMenu\\index.js",[],"C:\\Users\\titol\\Desktop\\projects\\paw-sible_pet\\client\\src\\components\\ProviderList\\index.js",[],"C:\\Users\\titol\\Desktop\\projects\\paw-sible_pet\\client\\src\\components\\CartReservation\\index.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":2,"column":16,"nodeType":"108","messageId":"109","endLine":2,"endColumn":24},{"ruleId":"106","severity":1,"message":"110","line":11,"column":12,"nodeType":"108","messageId":"109","endLine":11,"endColumn":26},{"ruleId":"106","severity":1,"message":"111","line":6,"column":8,"nodeType":"108","messa